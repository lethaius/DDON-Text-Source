<resource endian="Little" locale="0" unknown="0" tag="TextWeb">
  <message>
    <original xml:space="preserve">覚者よ――</original>
  </message>
  <message>
    <original xml:space="preserve">我に心臓を捧げしものよ――</original>
  </message>
  <message>
    <original xml:space="preserve">我が力は失われ――
動くこともままならぬ――</original>
  </message>
  <message>
    <original xml:space="preserve">我が命の行く末を――</original>
  </message>
  <message>
    <original xml:space="preserve">おまえたちに預けよう――</original>
  </message>
  <message>
    <original xml:space="preserve">このレスタニアを守ってくれ</original>
  </message>
  <message>
    <original xml:space="preserve">おのが目で、全てを見るのだ</original>
  </message>
</resource>