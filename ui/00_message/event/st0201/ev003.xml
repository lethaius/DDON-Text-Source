<resource endian="Little" locale="0" unknown="0" tag="TextWeb">
  <message>
    <text xml:space="preserve">覚者よ――</text>
  </message>
  <message>
    <text xml:space="preserve">我に心臓を捧げしものよ――</text>
  </message>
  <message>
    <text xml:space="preserve">我が力は失われ――
動くこともままならぬ――</text>
  </message>
  <message>
    <text xml:space="preserve">我が命の行く末を――</text>
  </message>
  <message>
    <text xml:space="preserve">おまえたちに預けよう――</text>
  </message>
  <message>
    <text xml:space="preserve">このレスタニアを守ってくれ</text>
  </message>
  <message>
    <text xml:space="preserve">おのが目で、全てを見るのだ</text>
  </message>
</resource>