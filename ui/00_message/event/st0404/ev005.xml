<resource endian="Little" locale="0" unknown="0" tag="TextWeb">
  <message>
    <text xml:space="preserve">――覚者さんたち、やるね</text>
  </message>
  <message>
    <text xml:space="preserve">もう、メイリーフは
――任せるよ</text>
  </message>
  <message>
    <text xml:space="preserve">ひとつ、頼みがある――</text>
  </message>
  <message>
    <text xml:space="preserve">まもなく尽きる――</text>
  </message>
  <message>
    <text xml:space="preserve">僕の命――</text>
  </message>
  <message>
    <text xml:space="preserve">あの器に入れて――
白竜に捧げて欲しい――</text>
  </message>
  <message>
    <text xml:space="preserve">きっときみたちの――</text>
  </message>
  <message>
    <text xml:space="preserve">役に立つはず――</text>
  </message>
  <message>
    <text xml:space="preserve">ああ――</text>
  </message>
  <message>
    <text xml:space="preserve">看取られて死ねるなんて――</text>
  </message>
  <message>
    <text xml:space="preserve">考えたこともなかった――</text>
  </message>
  <message>
    <text xml:space="preserve">これはいただくぞ！</text>
  </message>
  <message>
    <text xml:space="preserve">（高笑い）</text>
  </message>
</resource>