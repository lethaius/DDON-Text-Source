<resource endian="Little" locale="0" unknown="0" tag="TextWeb">
  <message>
    <text xml:space="preserve">目指しましょう！　共に勝利を！！</text>
  </message>
  <message>
    <text xml:space="preserve">最高のパートナーですね、マスター！</text>
  </message>
  <message>
    <text xml:space="preserve">なんと心強い！　負ける気がしません！</text>
  </message>
  <message>
    <text xml:space="preserve">共に行けること、誇りに思います！</text>
  </message>
  <message>
    <text xml:space="preserve">おお！　この出会いに感謝します！</text>
  </message>
  <message>
    <text xml:space="preserve">いざ行かん、試練の旅へ！</text>
  </message>
  <message>
    <text xml:space="preserve">おお！　あの方が来られたのでは？</text>
  </message>
  <message>
    <text xml:space="preserve">これぞ、熱意の成せる業！</text>
  </message>
  <message>
    <text xml:space="preserve">よしッ！　次もこの調子で！！</text>
  </message>
  <message>
    <text xml:space="preserve">我がマスターに栄光あれ！</text>
  </message>
  <message>
    <text xml:space="preserve">くっ――この失敗は次の糧に！</text>
  </message>
  <message>
    <text xml:space="preserve">努力のたまものです！</text>
  </message>
  <message>
    <text xml:space="preserve">我が力、尽きること無し！</text>
  </message>
  <message>
    <text xml:space="preserve">授かりし力、存分に奮う覚悟！</text>
  </message>
  <message>
    <text xml:space="preserve">どんな冒険が始まるのでしょうね、マスター！</text>
  </message>
  <message>
    <text xml:space="preserve">我らの行く手、さえぎるものなし！</text>
  </message>
  <message>
    <text xml:space="preserve">いた、そこか！</text>
  </message>
  <message>
    <text xml:space="preserve">敵発見、備えを！</text>
  </message>
  <message>
    <text xml:space="preserve">我がマスターの邪魔はさせるか！</text>
  </message>
  <message>
    <text xml:space="preserve">これは！　相手にとって不足なし！</text>
  </message>
  <message>
    <text xml:space="preserve">なに、必ず勝機はあるはずです！</text>
  </message>
  <message>
    <text xml:space="preserve">面目ありません！</text>
  </message>
  <message>
    <text xml:space="preserve">――っ、救援を！</text>
  </message>
  <message>
    <text xml:space="preserve">手を貸して下さい！</text>
  </message>
  <message>
    <text xml:space="preserve">む、無念――</text>
  </message>
  <message>
    <text xml:space="preserve">し、しまった――</text>
  </message>
  <message>
    <text xml:space="preserve">先手必勝！</text>
  </message>
  <message>
    <text xml:space="preserve">蹴散らしましょう！</text>
  </message>
  <message>
    <text xml:space="preserve">うむ、いい戦いでした！</text>
  </message>
  <message>
    <text xml:space="preserve">ははは！　我らの勝ちです！</text>
  </message>
  <message>
    <text xml:space="preserve">我がマスターに敵はなし！！</text>
  </message>
  <message>
    <text xml:space="preserve">マスター！！　今です！</text>
  </message>
  <message>
    <text xml:space="preserve">こんな傷など！　まだまだ余裕です</text>
  </message>
  <message>
    <text xml:space="preserve">これしきの傷で、引くものかっ！</text>
  </message>
  <message>
    <text xml:space="preserve">危機にこそ、チャンスがあります！</text>
  </message>
  <message>
    <text xml:space="preserve">ま、まだまだぁ！！</text>
  </message>
  <message>
    <text xml:space="preserve">うおおお、持ちこたえてみせる！！</text>
  </message>
  <message>
    <text xml:space="preserve">マ、マスタァアーー！！！</text>
  </message>
  <message>
    <text xml:space="preserve">はあああああっ！</text>
  </message>
  <message>
    <text xml:space="preserve">よし！　回復だ！</text>
  </message>
  <message>
    <text xml:space="preserve">こっちだぁ！　臆病者ども！</text>
  </message>
  <message>
    <text xml:space="preserve">何度でも助けます！</text>
  </message>
  <message>
    <text xml:space="preserve">そうはさせるか！</text>
  </message>
  <message>
    <text xml:space="preserve">ここで行く！</text>
  </message>
  <message>
    <text xml:space="preserve">受けてみろ！</text>
  </message>
  <message>
    <text xml:space="preserve">我が渾身の力！</text>
  </message>
  <message>
    <text xml:space="preserve">私を踏み台に！</text>
  </message>
  <message>
    <text xml:space="preserve">任せて下さい！</text>
  </message>
  <message>
    <text xml:space="preserve">了解です！</text>
  </message>
  <message>
    <text xml:space="preserve">う！　で、できません！</text>
  </message>
</resource>