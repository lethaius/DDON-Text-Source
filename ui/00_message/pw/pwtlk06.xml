<resource endian="Little" locale="0" unknown="0" tag="TextWeb">
  <message>
    <text xml:space="preserve">よぉし、ぼくも頑張ります！</text>
  </message>
  <message>
    <text xml:space="preserve">こんにちは！　一生懸命やりますね！</text>
  </message>
  <message>
    <text xml:space="preserve">一緒に冒険、楽しみです！　わくわくするなぁ</text>
  </message>
  <message>
    <text xml:space="preserve">わぁ、強そうですね！　よろしくお願いします！</text>
  </message>
  <message>
    <text xml:space="preserve">一番の家来を目指します、覚者さま！</text>
  </message>
  <message>
    <text xml:space="preserve">安心してください！　ぼくが守りますから！</text>
  </message>
  <message>
    <text xml:space="preserve">あ！　覚者さま、誰か来ましたよ？</text>
  </message>
  <message>
    <text xml:space="preserve">成功ですね！　覚者さま！</text>
  </message>
  <message>
    <text xml:space="preserve">やったぁ！　お役に立てて嬉しいなぁ</text>
  </message>
  <message>
    <text xml:space="preserve">この次もうまくいきますように！</text>
  </message>
  <message>
    <text xml:space="preserve">あ～、失敗かぁ。悔しいなぁ――</text>
  </message>
  <message>
    <text xml:space="preserve">ぼくの頑張り、見てて下さいね！</text>
  </message>
  <message>
    <text xml:space="preserve">あ！　なんだか今、強くなりました！</text>
  </message>
  <message>
    <text xml:space="preserve">覚者さまより強くなって、きっと守りますから！</text>
  </message>
  <message>
    <text xml:space="preserve">行き先はどこなんですか？　楽しみだな</text>
  </message>
  <message>
    <text xml:space="preserve">今日は何があるんだろう</text>
  </message>
  <message>
    <text xml:space="preserve">あっ！！　ほら、　敵ですよ！</text>
  </message>
  <message>
    <text xml:space="preserve">覚者さま、敵があんなところに！</text>
  </message>
  <message>
    <text xml:space="preserve">何かいる――気をつけないと――</text>
  </message>
  <message>
    <text xml:space="preserve">わ！　あれ、すごく強そうですよ！？</text>
  </message>
  <message>
    <text xml:space="preserve">覚者さま、あ、あんなのと戦うんですか？</text>
  </message>
  <message>
    <text xml:space="preserve">ありがとう！</text>
  </message>
  <message>
    <text xml:space="preserve">だ、誰か助けて！！</text>
  </message>
  <message>
    <text xml:space="preserve">こいつを何とかして！</text>
  </message>
  <message>
    <text xml:space="preserve">ごめんなさい、ぼく、もう――</text>
  </message>
  <message>
    <text xml:space="preserve">うっ――やられた――</text>
  </message>
  <message>
    <text xml:space="preserve">よぉし、ぼくがやっつけます！</text>
  </message>
  <message>
    <text xml:space="preserve">ああっ、敵が来ました！</text>
  </message>
  <message>
    <text xml:space="preserve">楽勝でしたね！</text>
  </message>
  <message>
    <text xml:space="preserve">思ったほど強くなかったなぁ</text>
  </message>
  <message>
    <text xml:space="preserve">覚者さまがご無事でよかったです！</text>
  </message>
  <message>
    <text xml:space="preserve">今です！　ほらっ！！</text>
  </message>
  <message>
    <text xml:space="preserve">心配ないですよ！　これくらい</text>
  </message>
  <message>
    <text xml:space="preserve">――ちょっとやられました</text>
  </message>
  <message>
    <text xml:space="preserve">痛たたたっ――でも頑張る――</text>
  </message>
  <message>
    <text xml:space="preserve">か、覚者さまぁ、ぼくもう体力が――</text>
  </message>
  <message>
    <text xml:space="preserve">ああ、だめ――倒れそう――</text>
  </message>
  <message>
    <text xml:space="preserve">え！？　覚者さま、覚者さまぁっ！！</text>
  </message>
  <message>
    <text xml:space="preserve">よーーーしこれでっ！</text>
  </message>
  <message>
    <text xml:space="preserve">あっ！　すぐに治します！</text>
  </message>
  <message>
    <text xml:space="preserve">おーい！　ぼくはこっちだ！</text>
  </message>
  <message>
    <text xml:space="preserve">支援しますね！</text>
  </message>
  <message>
    <text xml:space="preserve">当たらないよ！</text>
  </message>
  <message>
    <text xml:space="preserve">それっ！</text>
  </message>
  <message>
    <text xml:space="preserve">えいやーっ、とう！</text>
  </message>
  <message>
    <text xml:space="preserve">これは痛いぞ！！</text>
  </message>
  <message>
    <text xml:space="preserve">こっちです、さあ跳んで！</text>
  </message>
  <message>
    <text xml:space="preserve">はい、解りました！</text>
  </message>
  <message>
    <text xml:space="preserve">了解っ！</text>
  </message>
  <message>
    <text xml:space="preserve">えぇ！？　ちょっと無理です！</text>
  </message>
</resource>