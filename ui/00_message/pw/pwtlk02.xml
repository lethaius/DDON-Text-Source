<resource endian="Little" locale="0" unknown="0" tag="TextWeb">
  <message>
    <text xml:space="preserve">お役に立てるかどうか――</text>
  </message>
  <message>
    <text xml:space="preserve">できたらお供を――</text>
  </message>
  <message>
    <text xml:space="preserve">ご、ご一緒できて感激です</text>
  </message>
  <message>
    <text xml:space="preserve">自分のような者で良ければ――</text>
  </message>
  <message>
    <text xml:space="preserve">うまくやれるかどうか――</text>
  </message>
  <message>
    <text xml:space="preserve">どうか、旅が成功しますように――</text>
  </message>
  <message>
    <text xml:space="preserve">あの――誰か、来たみたいです</text>
  </message>
  <message>
    <text xml:space="preserve">なんとかなりましたね</text>
  </message>
  <message>
    <text xml:space="preserve">こんな自分でも、やれた！</text>
  </message>
  <message>
    <text xml:space="preserve">運を味方にできました</text>
  </message>
  <message>
    <text xml:space="preserve">落ち込みます――</text>
  </message>
  <message>
    <text xml:space="preserve">少しは強くなれたような気が――</text>
  </message>
  <message>
    <text xml:space="preserve">やった！　これできっと――</text>
  </message>
  <message>
    <text xml:space="preserve">少し、自信が湧きました</text>
  </message>
  <message>
    <text xml:space="preserve">緊張しますね</text>
  </message>
  <message>
    <text xml:space="preserve">どこまで行くんでしょう</text>
  </message>
  <message>
    <text xml:space="preserve">あっ、あそこに！</text>
  </message>
  <message>
    <text xml:space="preserve">敵が、もう近くに</text>
  </message>
  <message>
    <text xml:space="preserve">敵です、もっと慎重に</text>
  </message>
  <message>
    <text xml:space="preserve">あ、あれと戦うなんて――</text>
  </message>
  <message>
    <text xml:space="preserve">あの敵は――ま、まずいかも――</text>
  </message>
  <message>
    <text xml:space="preserve">た、助かりました</text>
  </message>
  <message>
    <text xml:space="preserve">助けて！！</text>
  </message>
  <message>
    <text xml:space="preserve">うわあっ、だ、誰かっ！</text>
  </message>
  <message>
    <text xml:space="preserve">ああ――力が、抜けていく――</text>
  </message>
  <message>
    <text xml:space="preserve">もう、もうダメかも――</text>
  </message>
  <message>
    <text xml:space="preserve">行くしかない、ですか</text>
  </message>
  <message>
    <text xml:space="preserve">仕方ありません</text>
  </message>
  <message>
    <text xml:space="preserve">つ、疲れましたね</text>
  </message>
  <message>
    <text xml:space="preserve">ああ――緊張しましたぁ</text>
  </message>
  <message>
    <text xml:space="preserve">ご無事で本当に良かったです</text>
  </message>
  <message>
    <text xml:space="preserve">止（と）めました！　今のうちに！</text>
  </message>
  <message>
    <text xml:space="preserve">何ともない、何ともない</text>
  </message>
  <message>
    <text xml:space="preserve">うう、不安です――</text>
  </message>
  <message>
    <text xml:space="preserve">こ、このままでは――</text>
  </message>
  <message>
    <text xml:space="preserve">もう、もうギリギリ――</text>
  </message>
  <message>
    <text xml:space="preserve">は、早く回復しないと！</text>
  </message>
  <message>
    <text xml:space="preserve">え？　――か、覚者さま！？</text>
  </message>
  <message>
    <text xml:space="preserve">待って、もう少し――</text>
  </message>
  <message>
    <text xml:space="preserve">これで少しは</text>
  </message>
  <message>
    <text xml:space="preserve">目立ちたくないけど、ここは――！</text>
  </message>
  <message>
    <text xml:space="preserve">が、頑張って！</text>
  </message>
  <message>
    <text xml:space="preserve">危ない！</text>
  </message>
  <message>
    <text xml:space="preserve">このっ！</text>
  </message>
  <message>
    <text xml:space="preserve">いい加減に！</text>
  </message>
  <message>
    <text xml:space="preserve">ああっ、もうっ！</text>
  </message>
  <message>
    <text xml:space="preserve">早く、今なら！</text>
  </message>
  <message>
    <text xml:space="preserve">うん――</text>
  </message>
  <message>
    <text xml:space="preserve">りょ、了解</text>
  </message>
  <message>
    <text xml:space="preserve">えっ！？　そんな、無理――</text>
  </message>
</resource>