<resource endian="Little" locale="0" unknown="0" tag="TextWeb">
  <message>
    <text xml:space="preserve">おう！　覚者様、よろしくお願いします</text>
  </message>
  <message>
    <text xml:space="preserve">どうも。遠慮しねぇで何でも言ってくださいよ</text>
  </message>
  <message>
    <text xml:space="preserve">俺の力、きっとお役に立ちますぜ</text>
  </message>
  <message>
    <text xml:space="preserve">どんな戦場でも喜んで</text>
  </message>
  <message>
    <text xml:space="preserve">こっちの準備ぁできてます。行きますか</text>
  </message>
  <message>
    <text xml:space="preserve">きっちりお守りしますんで、ご安心を</text>
  </message>
  <message>
    <text xml:space="preserve">おっ、この感じ。あの方が来ましたか</text>
  </message>
  <message>
    <text xml:space="preserve">ハッ！　何とかうまくいきましたね</text>
  </message>
  <message>
    <text xml:space="preserve">上出来だったと思いますがね</text>
  </message>
  <message>
    <text xml:space="preserve">結構な成果ですよ、こいつぁ！</text>
  </message>
  <message>
    <text xml:space="preserve">あぁ、下手うっちまいましたね。こりゃ</text>
  </message>
  <message>
    <text xml:space="preserve">お！？　悪くねぇ手ごたえだ</text>
  </message>
  <message>
    <text xml:space="preserve">うおおっ！　力が漲ってきやがる</text>
  </message>
  <message>
    <text xml:space="preserve">へっ！　次の戦闘、期待して下さいよ</text>
  </message>
  <message>
    <text xml:space="preserve">さぁてと、どっから行きましょう？</text>
  </message>
  <message>
    <text xml:space="preserve">ま、何かあったらその時考えましょう</text>
  </message>
  <message>
    <text xml:space="preserve">エモノですぜ</text>
  </message>
  <message>
    <text xml:space="preserve">敵ですよ、やりますか？</text>
  </message>
  <message>
    <text xml:space="preserve">敵か？　仕方ねぇ――！</text>
  </message>
  <message>
    <text xml:space="preserve">舐めてかかると、痛い目見そうだな。ありゃ</text>
  </message>
  <message>
    <text xml:space="preserve">チッ！　厄介そうなヤツですね</text>
  </message>
  <message>
    <text xml:space="preserve">すまねぇ！</text>
  </message>
  <message>
    <text xml:space="preserve">誰か、手ぇ貸してくれ！</text>
  </message>
  <message>
    <text xml:space="preserve">クッ！　離しやがれ、この！</text>
  </message>
  <message>
    <text xml:space="preserve">おい、冗談――だ――ろ――</text>
  </message>
  <message>
    <text xml:space="preserve">なっ――何ぃっ！？</text>
  </message>
  <message>
    <text xml:space="preserve">来やがったな！</text>
  </message>
  <message>
    <text xml:space="preserve">へっ、やりますか！</text>
  </message>
  <message>
    <text xml:space="preserve">お怪我ぁありませんでしたか？</text>
  </message>
  <message>
    <text xml:space="preserve">まっ、運動がわりにゃなりましたね</text>
  </message>
  <message>
    <text xml:space="preserve">覚者様の邪魔してんじゃねぇぞ、おう！</text>
  </message>
  <message>
    <text xml:space="preserve">よし、捕まえたぜ！！</text>
  </message>
  <message>
    <text xml:space="preserve">あぁ、大した怪我じゃありません</text>
  </message>
  <message>
    <text xml:space="preserve">楽勝ってわけにゃいきませんね</text>
  </message>
  <message>
    <text xml:space="preserve">つっ――気ぃ引き締めます――</text>
  </message>
  <message>
    <text xml:space="preserve">さすがにこりゃ、きついな――</text>
  </message>
  <message>
    <text xml:space="preserve">これ以上の無理ぁ、保障できませんよ</text>
  </message>
  <message>
    <text xml:space="preserve">おい！！　嘘だろっ、覚者様！</text>
  </message>
  <message>
    <text xml:space="preserve">食らわしてやる――</text>
  </message>
  <message>
    <text xml:space="preserve">おう、回復だ！</text>
  </message>
  <message>
    <text xml:space="preserve">どこ見てやがる、相手は俺だ！！</text>
  </message>
  <message>
    <text xml:space="preserve">よし、援護する！</text>
  </message>
  <message>
    <text xml:space="preserve">あぶねぇな！</text>
  </message>
  <message>
    <text xml:space="preserve">おう、うらぁッ！！</text>
  </message>
  <message>
    <text xml:space="preserve">食らいやがれッ！！</text>
  </message>
  <message>
    <text xml:space="preserve">そら、こいつでどうだ！</text>
  </message>
  <message>
    <text xml:space="preserve">こっちだ、打ち上げる！</text>
  </message>
  <message>
    <text xml:space="preserve">解ったぜ！</text>
  </message>
  <message>
    <text xml:space="preserve">おう、了解！</text>
  </message>
  <message>
    <text xml:space="preserve">そいつぁムリだ</text>
  </message>
</resource>