<resource endian="Little" locale="0" unknown="0" tag="TextWeb">
  <message>
    <text xml:space="preserve">素敵な方。お目にかかれて光栄よ</text>
  </message>
  <message>
    <text xml:space="preserve">良い旅になるように精一杯尽くすわ</text>
  </message>
  <message>
    <text xml:space="preserve">あたしで良ければ、何でも言いつけて</text>
  </message>
  <message>
    <text xml:space="preserve">サポートはお任せを。ふふっ</text>
  </message>
  <message>
    <text xml:space="preserve">気に入って頂けるよう努力するわ</text>
  </message>
  <message>
    <text xml:space="preserve">マスターのこと、よろしくお願いしますね</text>
  </message>
  <message>
    <text xml:space="preserve">あら。マスター、あの方が来たみたい</text>
  </message>
  <message>
    <text xml:space="preserve">さすがは、あたしのマスター</text>
  </message>
  <message>
    <text xml:space="preserve">うふふ！　いい成果でしたね</text>
  </message>
  <message>
    <text xml:space="preserve">この結果、あなたの実力よ</text>
  </message>
  <message>
    <text xml:space="preserve">こういうこともあるわ。気を落とさないで</text>
  </message>
  <message>
    <text xml:space="preserve">ふふっ、ちょっといい感じになったかしら</text>
  </message>
  <message>
    <text xml:space="preserve">みなぎってくる！</text>
  </message>
  <message>
    <text xml:space="preserve">いつまでも、お傍に置いて下さいね</text>
  </message>
  <message>
    <text xml:space="preserve">忘れ物はありません？　道具に、装備に――</text>
  </message>
  <message>
    <text xml:space="preserve">さぁ、行きましょうかマスター</text>
  </message>
  <message>
    <text xml:space="preserve">あら、敵よ</text>
  </message>
  <message>
    <text xml:space="preserve">気をつけて。敵の気配が</text>
  </message>
  <message>
    <text xml:space="preserve">何かいるわ、近くに</text>
  </message>
  <message>
    <text xml:space="preserve">マスター、あれを相手にするの？</text>
  </message>
  <message>
    <text xml:space="preserve">戦って大丈夫？　かなり手ごわそうよ</text>
  </message>
  <message>
    <text xml:space="preserve">ん、ありがとう！</text>
  </message>
  <message>
    <text xml:space="preserve">あぁっ、助けて！！</text>
  </message>
  <message>
    <text xml:space="preserve">ちっ、油断したわ――！</text>
  </message>
  <message>
    <text xml:space="preserve">あぁ――こんなはずじゃ――</text>
  </message>
  <message>
    <text xml:space="preserve">う！　ごめんなさい、マスター！</text>
  </message>
  <message>
    <text xml:space="preserve">さ、始めるわよ</text>
  </message>
  <message>
    <text xml:space="preserve">気合入れていきましょ！</text>
  </message>
  <message>
    <text xml:space="preserve">もっと満足させて頂戴</text>
  </message>
  <message>
    <text xml:space="preserve">ふふ、なかなか良かったわ</text>
  </message>
  <message>
    <text xml:space="preserve">マスターに刃を向ける者は、あたしが許さない</text>
  </message>
  <message>
    <text xml:space="preserve">さぁ、お願い！　抑えてる間に！</text>
  </message>
  <message>
    <text xml:space="preserve">この程度、何でもないわ</text>
  </message>
  <message>
    <text xml:space="preserve">まずいわね――何とかしなくちゃ</text>
  </message>
  <message>
    <text xml:space="preserve">ちょっと辛くなってきたわ――</text>
  </message>
  <message>
    <text xml:space="preserve">うっ！　今は、倒れるわけにはいかない――</text>
  </message>
  <message>
    <text xml:space="preserve">何とか――耐えなくちゃ――！</text>
  </message>
  <message>
    <text xml:space="preserve">ねぇちょっと！？　　だめよ、マスター！</text>
  </message>
  <message>
    <text xml:space="preserve">いくわよっ！</text>
  </message>
  <message>
    <text xml:space="preserve">大丈夫、すぐに癒すわ</text>
  </message>
  <message>
    <text xml:space="preserve">そこのあなた！　あたしを見なさい！</text>
  </message>
  <message>
    <text xml:space="preserve">サポートは任せて！</text>
  </message>
  <message>
    <text xml:space="preserve">やめなさい！</text>
  </message>
  <message>
    <text xml:space="preserve">これでどう？</text>
  </message>
  <message>
    <text xml:space="preserve">はっ！　倒れなさい！</text>
  </message>
  <message>
    <text xml:space="preserve">しつこいのよ！！</text>
  </message>
  <message>
    <text xml:space="preserve">あたしに乗って！　早く！！</text>
  </message>
  <message>
    <text xml:space="preserve">解ったわ！</text>
  </message>
  <message>
    <text xml:space="preserve">仰せのままに</text>
  </message>
  <message>
    <text xml:space="preserve">え！？　急に言われても――</text>
  </message>
</resource>