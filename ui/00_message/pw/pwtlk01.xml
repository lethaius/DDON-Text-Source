<resource endian="Little" locale="0" unknown="0" tag="TextWeb">
  <message>
    <text xml:space="preserve">よろしくお願いします</text>
  </message>
  <message>
    <text xml:space="preserve">ともに参りましょう</text>
  </message>
  <message>
    <text xml:space="preserve">お会いできて光栄です</text>
  </message>
  <message>
    <text xml:space="preserve">良き旅になるよう、全力を尽くします</text>
  </message>
  <message>
    <text xml:space="preserve">どうか、私の力を存分にお使いください</text>
  </message>
  <message>
    <text xml:space="preserve">我らが竜とともに！</text>
  </message>
  <message>
    <text xml:space="preserve">ご友人がお見えです</text>
  </message>
  <message>
    <text xml:space="preserve">覚者さま、お見事です</text>
  </message>
  <message>
    <text xml:space="preserve">やりましたね！</text>
  </message>
  <message>
    <text xml:space="preserve">我が主に竜の加護があらんことを</text>
  </message>
  <message>
    <text xml:space="preserve">うまくいきませんね――</text>
  </message>
  <message>
    <text xml:space="preserve">いい手ごたえです</text>
  </message>
  <message>
    <text xml:space="preserve">力が湧いてきました！</text>
  </message>
  <message>
    <text xml:space="preserve">一層お役に立ってみせます！</text>
  </message>
  <message>
    <text xml:space="preserve">さあ、行きましょう</text>
  </message>
  <message>
    <text xml:space="preserve">いよいよですね</text>
  </message>
  <message>
    <text xml:space="preserve">敵を発見！</text>
  </message>
  <message>
    <text xml:space="preserve">あそこに敵がいます！</text>
  </message>
  <message>
    <text xml:space="preserve">いました、ご注意を！</text>
  </message>
  <message>
    <text xml:space="preserve">あの敵は、まずいですね――</text>
  </message>
  <message>
    <text xml:space="preserve">強敵です、警戒を！</text>
  </message>
  <message>
    <text xml:space="preserve">感謝します！</text>
  </message>
  <message>
    <text xml:space="preserve">早く、助けを！</text>
  </message>
  <message>
    <text xml:space="preserve">助けてください！</text>
  </message>
  <message>
    <text xml:space="preserve">し、しくじりました――！</text>
  </message>
  <message>
    <text xml:space="preserve">くっ――力が――！</text>
  </message>
  <message>
    <text xml:space="preserve">油断は禁物です！</text>
  </message>
  <message>
    <text xml:space="preserve">さあ、やりましょう！</text>
  </message>
  <message>
    <text xml:space="preserve">倒しました！</text>
  </message>
  <message>
    <text xml:space="preserve">我々の勝利です！</text>
  </message>
  <message>
    <text xml:space="preserve">生き延びたようです</text>
  </message>
  <message>
    <text xml:space="preserve">今です、覚者さま！</text>
  </message>
  <message>
    <text xml:space="preserve">まだ充分、戦えます</text>
  </message>
  <message>
    <text xml:space="preserve">厳しい状況ですね</text>
  </message>
  <message>
    <text xml:space="preserve">このままでは不利かもしれません</text>
  </message>
  <message>
    <text xml:space="preserve">深手を――負いましたっ――</text>
  </message>
  <message>
    <text xml:space="preserve">体力が、もう――！</text>
  </message>
  <message>
    <text xml:space="preserve">そんな、まさか――！</text>
  </message>
  <message>
    <text xml:space="preserve">これでっ――！</text>
  </message>
  <message>
    <text xml:space="preserve">回復します</text>
  </message>
  <message>
    <text xml:space="preserve">敵を引きつけます！</text>
  </message>
  <message>
    <text xml:space="preserve">これで有利になるはずです</text>
  </message>
  <message>
    <text xml:space="preserve">回避します！</text>
  </message>
  <message>
    <text xml:space="preserve">喰らえ！</text>
  </message>
  <message>
    <text xml:space="preserve">狙います！</text>
  </message>
  <message>
    <text xml:space="preserve">逃さない！</text>
  </message>
  <message>
    <text xml:space="preserve">こっちです！</text>
  </message>
  <message>
    <text xml:space="preserve">はい！</text>
  </message>
  <message>
    <text xml:space="preserve">承知しました！</text>
  </message>
  <message>
    <text xml:space="preserve">従えません</text>
  </message>
</resource>