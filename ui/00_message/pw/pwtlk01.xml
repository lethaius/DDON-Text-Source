<resource endian="Little" locale="0" unknown="0" tag="TextWeb">
  <message>
    <original xml:space="preserve">よろしくお願いします</original>
  </message>
  <message>
    <original xml:space="preserve">ともに参りましょう</original>
  </message>
  <message>
    <original xml:space="preserve">お会いできて光栄です</original>
  </message>
  <message>
    <original xml:space="preserve">良き旅になるよう、全力を尽くします</original>
  </message>
  <message>
    <original xml:space="preserve">どうか、私の力を存分にお使いください</original>
  </message>
  <message>
    <original xml:space="preserve">我らが竜とともに！</original>
  </message>
  <message>
    <original xml:space="preserve">ご友人がお見えです</original>
  </message>
  <message>
    <original xml:space="preserve">覚者さま、お見事です</original>
  </message>
  <message>
    <original xml:space="preserve">やりましたね！</original>
  </message>
  <message>
    <original xml:space="preserve">我が主に竜の加護があらんことを</original>
  </message>
  <message>
    <original xml:space="preserve">うまくいきませんね――</original>
  </message>
  <message>
    <original xml:space="preserve">いい手ごたえです</original>
  </message>
  <message>
    <original xml:space="preserve">力が湧いてきました！</original>
  </message>
  <message>
    <original xml:space="preserve">一層お役に立ってみせます！</original>
  </message>
  <message>
    <original xml:space="preserve">さあ、行きましょう</original>
  </message>
  <message>
    <original xml:space="preserve">いよいよですね</original>
  </message>
  <message>
    <original xml:space="preserve">敵を発見！</original>
  </message>
  <message>
    <original xml:space="preserve">あそこに敵がいます！</original>
  </message>
  <message>
    <original xml:space="preserve">いました、ご注意を！</original>
  </message>
  <message>
    <original xml:space="preserve">あの敵は、まずいですね――</original>
  </message>
  <message>
    <original xml:space="preserve">強敵です、警戒を！</original>
  </message>
  <message>
    <original xml:space="preserve">感謝します！</original>
  </message>
  <message>
    <original xml:space="preserve">早く、助けを！</original>
  </message>
  <message>
    <original xml:space="preserve">助けてください！</original>
  </message>
  <message>
    <original xml:space="preserve">し、しくじりました――！</original>
  </message>
  <message>
    <original xml:space="preserve">くっ――力が――！</original>
  </message>
  <message>
    <original xml:space="preserve">油断は禁物です！</original>
  </message>
  <message>
    <original xml:space="preserve">さあ、やりましょう！</original>
  </message>
  <message>
    <original xml:space="preserve">倒しました！</original>
  </message>
  <message>
    <original xml:space="preserve">我々の勝利です！</original>
  </message>
  <message>
    <original xml:space="preserve">生き延びたようです</original>
  </message>
  <message>
    <original xml:space="preserve">今です、覚者さま！</original>
  </message>
  <message>
    <original xml:space="preserve">まだ充分、戦えます</original>
  </message>
  <message>
    <original xml:space="preserve">厳しい状況ですね</original>
  </message>
  <message>
    <original xml:space="preserve">このままでは不利かもしれません</original>
  </message>
  <message>
    <original xml:space="preserve">深手を――負いましたっ――</original>
  </message>
  <message>
    <original xml:space="preserve">体力が、もう――！</original>
  </message>
  <message>
    <original xml:space="preserve">そんな、まさか――！</original>
  </message>
  <message>
    <original xml:space="preserve">これでっ――！</original>
  </message>
  <message>
    <original xml:space="preserve">回復します</original>
  </message>
  <message>
    <original xml:space="preserve">敵を引きつけます！</original>
  </message>
  <message>
    <original xml:space="preserve">これで有利になるはずです</original>
  </message>
  <message>
    <original xml:space="preserve">回避します！</original>
  </message>
  <message>
    <original xml:space="preserve">喰らえ！</original>
  </message>
  <message>
    <original xml:space="preserve">狙います！</original>
  </message>
  <message>
    <original xml:space="preserve">逃さない！</original>
  </message>
  <message>
    <original xml:space="preserve">こっちです！</original>
  </message>
  <message>
    <original xml:space="preserve">はい！</original>
  </message>
  <message>
    <original xml:space="preserve">承知しました！</original>
  </message>
  <message>
    <original xml:space="preserve">従えません</original>
  </message>
</resource>